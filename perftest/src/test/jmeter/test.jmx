              <elementProp name="base_branch" elementType="Argument">
                <stringProp name="Argument.name">base_branch</stringProp>
                <stringProp name="Argument.value">main</stringProp>
                <stringProp name="Argument.metadata">=</stringProp>
              </elementProp>
              <elementProp name="table" elementType="Argument">
                <stringProp name="Argument.name">table</stringProp>
                <stringProp name="Argument.value">table-${table}</stringProp>
                <stringProp name="Argument.metadata">=</stringProp>
              </elementProp>
            </collectionProp>
          </elementProp>
          <stringProp name="classname">org.projectnessie.perftest.NessieSampler</stringProp>
